{
  "name": "date-picker-feature",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/date-picker/feature/src",
  "prefix": "lib",
  "projectType": "library",
  "release": {
    "version": {
      "manifestRootsToUpdate": [
        "dist/{projectRoot}"
      ],
      "currentVersionResolver": "git-tag",
      "fallbackCurrentVersionResolver": "disk"
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": [
        "{workspaceRoot}/dist/{projectRoot}"
      ],
      "options": {
        "project": "libs/shared/date-picker/feature/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/shared/date-picker/feature/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/shared/date-picker/feature/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/shared/date-picker/feature/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "syncVersions": false,
        "push": true,
        "pushTags": true,
        "trackDeps": true,
        "baseBranch": "develop",
        "postTargets": [
          "build",
          "publish"
        ]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          "cd dist/libs/shared/date-picker/feature && npm publish"
        ]
      },
      "dependsOn": [
        "build"
      ],
      "parallelism": false
    }
  }
}
