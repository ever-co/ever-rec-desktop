{
  "name": "database-electron",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/electron/database/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/electron/database",
        "main": "libs/electron/database/src/index.ts",
        "tsConfig": "libs/electron/database/tsconfig.lib.json",
        "assets": ["libs/electron/database/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/electron/database/jest.config.ts"
      }
    }
  }
}
