{
  "name": "electron-utils",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/electron/utils/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/electron/utils",
        "main": "libs/electron/utils/src/index.ts",
        "tsConfig": "libs/electron/utils/tsconfig.lib.json",
        "assets": [
          "libs/electron/utils/*.md"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/electron/utils/jest.config.ts"
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "syncVersions": false,
        "push": true,
        "pushTags": true,
        "trackDeps": true
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          "cd dist/libs/electron/utils && npm publish --access public"
        ]
      },
      "dependsOn": [
        "version",
        "build"
      ],
      "parallelism": false
    }
  }
}
