{
  "name": "window",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/electron/window/src",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist/{projectRoot}",
        "currentVersionResolver": "git-tag",
        "fallbackCurrentVersionResolver": "disk"
      }
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/electron/window",
        "main": "libs/electron/window/src/index.ts",
        "tsConfig": "libs/electron/window/tsconfig.lib.json",
        "assets": [
          "libs/electron/window/*.md"
        ]
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/electron/window/jest.config.ts"
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "syncVersions": false,
        "push": true,
        "pushTags": true,
        "trackDeps": true,
        "baseBranch": "develop",
        "postTargets": [
          "build",
          "publish"
        ]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          "cd dist/libs/electron/window && npm publish"
        ]
      },
      "dependsOn": [
        "build"
      ],
      "parallelism": false
    }
  }
}
